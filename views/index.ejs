<html>
  <head>
    <link rel="stylesheet" href="/styles.css">
    <title><%= title %></title>
  </head>
  <body>
    <div class="total">
        <%- include('links', {connect: connect}) %>
        <div class="outer">
          <% messages.forEach((message) => { %>
              <div class="container">
                  <p><%= message.text %></p>
                  <p><%= message.user %></p>
                  <p><%= message.added %></p>
              </div>
          <% }) %>
        </div>
    </div>
  </body>
</html>